    "watch:js": "parcel watch src/assets/js/index.js --out-dir dist/assets/js",
    "serve:site": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
