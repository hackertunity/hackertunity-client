  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run watch:js\" \"npm run serve:site\"",
    "build:js": "parcel build src/assets/js/index.js --out-dir dist/assets/js",
    "build:site": "cross-env ELEVENTY_ENV=prod eleventy",
