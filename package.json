  },
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.1.5",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "esm": "^3.2.25",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
